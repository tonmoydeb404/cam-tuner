{
  "name": "CamTuner",
  "version": "1.0.0",
  "{{chrome}}.manifest_version": 3,
  "{{firefox}}.manifest_version": 2,
  "description": "Customize your webcam configuration.",
  "background": {
    "service_worker": "src/background.ts"
  },
  "{{chrome}}.action": {
    "default_popup": "index.html"
  },
  "{{firefox}}.browser_action": {
    "default_popup": "index.html"
  }
}